<h1>Log In</h1>

<form>
  <div class="form-group">
    {{input value=email id="email" class="form-control"}}
  </div>

  <div class="form-group password-input">
    {{input value=password id="password" class="form-control"}}
  </div>

  <button class="submit btn btn-success" {{action "authenticate"}}>Log In</button>

</form>
